@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:v7VZRp a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "Q18-4-1" ;
    rdfs:comment "primary oocyte細胞における遺伝子ごとの発現量は？"@ja ;
    sh:select """# Endpoint: https://www.bgee.org/sparql
# Description: primary oocyte細胞における遺伝子ごとの発現量は？
# Parameter: anatomical_entity_name: (example: primary oocyte)

PREFIX genex: <http://purl.org/genex#>
PREFIX orth: <http://purl.org/net/orth#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT DISTINCT ?expression_level ?gene_name 
WHERE {
    VALUES ?anatomical_entity_name { "primary oocyte" }
    ?Expression a genex:Expression ;
        genex:hasExpressionLevel ?expression_level ;
        genex:hasSequenceUnit / rdfs:label ?gene_name ;
        genex:hasExpressionCondition / genex:hasAnatomicalEntity / rdfs:label ?anatomical_entity_name .
}
LIMIT 100
""" ;
    schema:target "https://bgee.org/sparql".
