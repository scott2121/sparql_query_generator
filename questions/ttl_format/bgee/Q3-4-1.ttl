@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:TMTC87 a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "Q3-4-1" ;
    rdfs:comment "APOC1遺伝子のFDR p値はいくらですか？"@ja ;
    sh:select """# Endpoint: https://www.bgee.org/sparql
# Description: APOC1遺伝子のFDR p値はいくらですか？
# Parameter: gene_name: (example: APOC1)

PREFIX genex: <http://purl.org/genex#>
PREFIX orth: <http://purl.org/net/orth#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT DISTINCT ?expression_fdr_pvalue 
WHERE {
    VALUES ?gene_name { "APOC1" }
    ?Expression a genex:Expression ;
        genex:hasFDRpvalue ?expression_fdr_pvalue ;
        genex:hasSequenceUnit / rdfs:label ?gene_name .
}
LIMIT 100
""" ;
    schema:target "https://bgee.org/sparql".
