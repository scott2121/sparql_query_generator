@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:1MpyQa a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "Q11-5-1" ;
    rdfs:comment "primary oocyteの解剖学的実体について説明してください。"@ja ;
    sh:select """# Endpoint: https://www.bgee.org/sparql
# Description: primary oocyteの解剖学的実体について説明してください。
# Parameter: anatomical_entity_name: (example: primary oocyte)

PREFIX genex: <http://purl.org/genex#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT DISTINCT ?anatomical_entity_description 
WHERE {
    VALUES ?anatomical_entity_name { "primary oocyte" }
    ?AnatomicalEntity a genex:AnatomicalEntity ;
        dcterms:description ?anatomical_entity_description ;
        rdfs:label ?anatomical_entity_name .
}
LIMIT 100
""" ;
    schema:target "https://bgee.org/sparql".
