@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:bGvDEy a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "Q21-6-2" ;
    rdfs:comment "D-tryptophan分子が参加する化学反応のRHEA IDと参考文献を示してください。"@ja ;
    sh:select """# Endpoint: https://sparql.rhea-db.org/sparql
# Description: D-tryptophan分子が参加する化学反応のRHEA IDと参考文献を示してください。
# Parameter: compound_name: (example: D-tryptophan)

PREFIX rhea: <http://rdf.rhea-db.org/>

SELECT DISTINCT ?Reaction ?reaction_citation 
WHERE {
    VALUES ?compound_name { "D-tryptophan" }
    ?Reaction rhea:citation ?reaction_citation ;
        rhea:side / rhea:contains / rhea:compound / rhea:name ?compound_name .
}
LIMIT 100
""" ;
    schema:target "https://sparql.rhea-db.org/sparql".
