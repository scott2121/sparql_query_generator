@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:5fhBQs a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "Q13-6-1" ;
    rdfs:comment "rhea:20105のRHEA番号を持つ反応を触媒する酵素のEC番号は何ですか？"@ja ;
    sh:select """# Endpoint: https://sparql.rhea-db.org/sparql
# Description: rhea:20105のRHEA番号を持つ反応を触媒する酵素のEC番号は何ですか？
# Parameter: Reaction: (example: rhea:20105)

PREFIX rhea: <http://rdf.rhea-db.org/>

SELECT DISTINCT ?reaction_ec_number 
WHERE {
    VALUES ?Reaction { rhea:20105 }
    ?Reaction rhea:ec ?reaction_ec_number .
}
LIMIT 100
""" ;
    schema:target "https://sparql.rhea-db.org/sparql".
