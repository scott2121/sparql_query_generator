@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:3OrQI7 a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "Q11-5-1" ;
    rdfs:comment "rhea:10024反応に関与する各分子の正味の電荷は正、負、それとも中性ですか、分子名とともに教えてください？"@ja ;
    sh:select """# Endpoint: https://sparql.rhea-db.org/sparql
# Description: rhea:10024反応に関与する各分子の正味の電荷は正、負、それとも中性ですか、分子名とともに教えてください？
# Parameter: Reaction: (example: rhea:10024)

PREFIX rhea: <http://rdf.rhea-db.org/>

SELECT DISTINCT ?compound_charge ?compound_name 
WHERE {
    VALUES ?Reaction { rhea:10024 }
    ?Reaction rhea:side / rhea:contains / rhea:compound / rhea:charge ?compound_charge ;
        rhea:side / rhea:contains / rhea:compound / rhea:name ?compound_name .
}
LIMIT 100
""" ;
    schema:target "https://sparql.rhea-db.org/sparql".
