@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:szOzUM a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "Q14-5-1" ;
    rdfs:comment "D-tryptophanという分子のChebi IDを教えてください。"@ja ;
    sh:select """# Endpoint: https://sparql.rhea-db.org/sparql
# Description: D-tryptophanという分子のChebi IDを教えてください。
# Parameter: compound_name: (example: D-tryptophan)

PREFIX rhea: <http://rdf.rhea-db.org/>

SELECT DISTINCT ?compound_chebi 
WHERE {
    VALUES ?compound_name { "D-tryptophan" }
    ?Compound rhea:chebi ?compound_chebi ;
        rhea:name ?compound_name .
}
LIMIT 100
""" ;
    schema:target "https://sparql.rhea-db.org/sparql".
