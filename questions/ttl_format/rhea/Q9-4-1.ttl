@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:YWRh3y a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "Q9-4-1" ;
    rdfs:comment "rhea:10024の反応に関連する論文のPubMed IDを教えてください"@ja ;
    sh:select """# Endpoint: https://sparql.rhea-db.org/sparql
# Description: rhea:10024の反応に関連する論文のPubMed IDを教えてください
# Parameter: Reaction: (example: rhea:10024)

PREFIX rhea: <http://rdf.rhea-db.org/>

SELECT DISTINCT ?reaction_citation 
WHERE {
    VALUES ?Reaction { rhea:10024 }
    ?Reaction rhea:citation ?reaction_citation .
}
LIMIT 100
""" ;
    schema:target "https://sparql.rhea-db.org/sparql".
