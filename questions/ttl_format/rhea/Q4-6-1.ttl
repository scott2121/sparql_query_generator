@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:4ISqOg a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "Q4-6-1" ;
    rdfs:comment "uniprot_enzyme:7.1.3.1のEC番号を持つ酵素が触媒する反応のRHEA番号は何ですか？"@ja ;
    sh:select """# Endpoint: https://sparql.rhea-db.org/sparql
# Description: uniprot_enzyme:7.1.3.1のEC番号を持つ酵素が触媒する反応のRHEA番号は何ですか？
# Parameter: reaction_ec_number: (example: uniprot_enzyme:7.1.3.1)

PREFIX rhea: <http://rdf.rhea-db.org/>
PREFIX uniprot_enzyme: <http://purl.uniprot.org/enzyme/>

SELECT DISTINCT ?Reaction 
WHERE {
    VALUES ?reaction_ec_number { uniprot_enzyme:7.1.3.1 }
    ?Reaction rhea:ec ?reaction_ec_number .
}
LIMIT 100
""" ;
    schema:target "https://sparql.rhea-db.org/sparql".
