@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:PQIWvk a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "Q2-5-1" ;
    rdfs:comment "rhea:10024の反応に参加する分子のCHEBI識別子は何ですか？"@ja ;
    sh:select """# Endpoint: https://sparql.rhea-db.org/sparql
# Description: rhea:10024の反応に参加する分子のCHEBI識別子は何ですか？
# Parameter: Reaction: (example: rhea:10024)

PREFIX rhea: <http://rdf.rhea-db.org/>

SELECT DISTINCT ?compound_chebi 
WHERE {
    VALUES ?Reaction { rhea:10024 }
    ?Reaction rhea:side / rhea:contains / rhea:compound / rhea:chebi ?compound_chebi .
}
LIMIT 100
""" ;
    schema:target "https://sparql.rhea-db.org/sparql".
