@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:96lyqb a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "ID1-5-1" ;
    rdfs:comment "Angiotensin-converting enzyme 2として知られるタンパク質のUniProtエントリー番号を探しています。"@ja ;
    sh:select """# Endpoint: https://rdfportal.org/sib/sparql
#           https://sparql.uniprot.org/sparql
# Description: Angiotensin-converting enzyme 2として知られるタンパク質のUniProtエントリー番号を探しています。
# Parameter: uniprot_recommended_name: (example: Angiotensin-converting enzyme 2)

PREFIX core: <http://purl.uniprot.org/core/>
PREFIX chembl: <http://rdf.ebi.ac.uk/terms/chembl#>

SELECT DISTINCT ?UniProt 
WHERE {
    VALUES ?uniprot_recommended_name { "Angiotensin-converting enzyme 2" }
    VALUES ?UniProt__class { core:Protein chembl:UniprotRef }
    ?UniProt a ?UniProt__class ;
        core:recommendedName / core:fullName ?uniprot_recommended_name .
}
LIMIT 100
""" ;
    schema:target "https://sparql.uniprot.org/sparql".
