@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:lrZWyB a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "ID17-6-1" ;
    rdfs:comment "ACE2遺伝子産物の結合部位において、リガンドが結合する配列の開始位置と終了位置を教えてください。"@ja ;
    sh:select """# Endpoint: https://rdfportal.org/sib/sparql
#           https://sparql.uniprot.org/sparql
# Description: ACE2遺伝子産物の結合部位において、リガンドが結合する配列の開始位置と終了位置を教えてください。
# Parameter: uniprot_gene_name: (example: ACE2)

PREFIX core: <http://purl.uniprot.org/core/>
PREFIX chembl: <http://rdf.ebi.ac.uk/terms/chembl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX faldo: <http://biohackathon.org/resource/faldo#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

SELECT DISTINCT ?binding_site_annotation_ligand ?binding_site_annotation_begin_position ?binding_site_annotation_end_position 
WHERE {
    VALUES ?uniprot_gene_name { "ACE2" }
    VALUES ?UniProt__class { core:Protein chembl:UniprotRef }
    ?UniProt a ?UniProt__class ;
        core:encodedBy / skos:prefLabel ?uniprot_gene_name .
    OPTIONAL {
        ?UniProt core:annotation / core:ligand / rdfs:subClassOf ?binding_site_annotation_ligand .
    }
    OPTIONAL {
        ?UniProt core:annotation ?BindingSiteAnnotation .
        ?BindingSiteAnnotation a core:Binding_Site_Annotation ;
            core:range [
                a faldo:Region ;
                faldo:begin [
                    a faldo:Position ;
                    faldo:position ?binding_site_annotation_begin_position ;
                ] ;
                faldo:end [
                    a faldo:Position ;
                    faldo:position ?binding_site_annotation_end_position ;
                ] ;
            ] .
    }
}
LIMIT 100
""" ;
    schema:target "https://sparql.uniprot.org/sparql".
