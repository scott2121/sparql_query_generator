@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:G3f58U a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "ID10-5-1" ;
    rdfs:comment "Reactomeデータベースでreactome:R-HSA-2022377という識別子に紐づけられているタンパク質の名前を教えてください。"@ja ;
    sh:select """# Endpoint: https://rdfportal.org/sib/sparql
#           https://sparql.uniprot.org/sparql
# Description: Reactomeデータベースでreactome:R-HSA-2022377という識別子に紐づけられているタンパク質の名前を教えてください。
# Parameter: uniprot_reactome: (example: reactome:R-HSA-2022377)

PREFIX core: <http://purl.uniprot.org/core/>
PREFIX chembl: <http://rdf.ebi.ac.uk/terms/chembl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX reactome: <http://identifiers.org/reactome/>

SELECT DISTINCT ?uniprot_recommended_name 
WHERE {
    VALUES ?uniprot_reactome { reactome:R-HSA-2022377 }
    VALUES ?UniProt__class { core:Protein chembl:UniprotRef }
    ?UniProt a ?UniProt__class ;
        core:recommendedName / core:fullName ?uniprot_recommended_name .
    OPTIONAL {
        ?UniProt rdfs:seeAlso ?uniprot_reactome .
        ?uniprot_reactome a core:Resource .
    }
}
LIMIT 100
""" ;
    schema:target "https://sparql.uniprot.org/sparql".
