@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:uVqLWh a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "ID25-4-1" ;
    rdfs:comment "angiotensin II + H2O = angiotensin-(1-7) + L-phenylalanineという反応を触媒するタンパク質の名前は何ですか?"@ja ;
    sh:select """# Endpoint: https://rdfportal.org/sib/sparql
#           https://sparql.uniprot.org/sparql
# Description: angiotensin II + H2O = angiotensin-(1-7) + L-phenylalanineという反応を触媒するタンパク質の名前は何ですか?
# Parameter: rhea_label: (example: angiotensin II + H2O = angiotensin-(1-7) + L-phenylalanine)

PREFIX core: <http://purl.uniprot.org/core/>
PREFIX chembl: <http://rdf.ebi.ac.uk/terms/chembl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT DISTINCT ?uniprot_recommended_name 
WHERE {
    VALUES ?rhea_label { "angiotensin II + H2O = angiotensin-(1-7) + L-phenylalanine" }
    VALUES ?UniProt__class { core:Protein chembl:UniprotRef }
    ?UniProt a ?UniProt__class ;
        core:recommendedName / core:fullName ?uniprot_recommended_name .
    OPTIONAL {
        ?UniProt core:annotation / core:catalyticActivity / core:catalyzedReaction / rdfs:label ?rhea_label .
    }
}
LIMIT 100
""" ;
    schema:target "https://sparql.uniprot.org/sparql".
