@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:zbxlf0 a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "ID22-6-1" ;
    rdfs:comment "TRP4.8遺伝子の自然変異についてわかっていることを説明していただけますか?"@ja ;
    sh:select """# Endpoint: https://rdfportal.org/sib/sparql
#           https://sparql.uniprot.org/sparql
# Description: TRP4.8遺伝子の自然変異についてわかっていることを説明していただけますか?
# Parameter: uniprot_gene_name: (example: TRP4.8)

PREFIX core: <http://purl.uniprot.org/core/>
PREFIX chembl: <http://rdf.ebi.ac.uk/terms/chembl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

SELECT DISTINCT ?natural_variant_annotation_desc 
WHERE {
    VALUES ?uniprot_gene_name { "TRP4.8" }
    VALUES ?UniProt__class { core:Protein chembl:UniprotRef }
    ?UniProt a ?UniProt__class ;
        core:encodedBy / skos:prefLabel ?uniprot_gene_name .
    OPTIONAL {
        ?UniProt core:annotation ?NaturalVariantAnnotation .
        ?NaturalVariantAnnotation a core:Natural_Variant_Annotation ;
            rdfs:comment ?natural_variant_annotation_desc .
    }
}
LIMIT 100
""" ;
    schema:target "https://sparql.uniprot.org/sparql".
