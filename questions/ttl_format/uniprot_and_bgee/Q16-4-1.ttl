@prefix ex: <https://is.gd/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
ex:ZeNiqE a sh:SPARQLSelectExecutable, sh:SPARQLExecutable ;
    rdfs:label "Q16-4-1" ;
    rdfs:comment "angiotensin II + H2O = angiotensin-(1-7) + L-phenylalanine という反応を触媒するタンパク質の名称は何ですか？その対応する遺伝子はどの性別で発現していますか？"@ja ;
    sh:select """# Endpoint: https://rdfportal.org/sib/sparql
# Description: angiotensin II + H2O = angiotensin-(1-7) + L-phenylalanine という反応を触媒するタンパク質の名称は何ですか？その対応する遺伝子はどの性別で発現していますか？
# Parameter: rhea_label: (example: angiotensin II + H2O = angiotensin-(1-7) + L-phenylalanine)

PREFIX genex: <http://purl.org/genex#>
PREFIX orth: <http://purl.org/net/orth#>
PREFIX lscr: <http://purl.org/lscr#>
PREFIX core: <http://purl.uniprot.org/core/>
PREFIX chembl: <http://rdf.ebi.ac.uk/terms/chembl#>
PREFIX corecatalyticactivity: <http://purl.uniprot.org/core/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT DISTINCT ?uniprot_recommended_name ?expression_condition_sex 
WHERE {
    VALUES ?rhea_label { "angiotensin II + H2O = angiotensin-(1-7) + L-phenylalanine" }
    ?Expression a genex:Expression ;
        genex:hasSequenceUnit / lscr:xrefUniprot / core:recommendedName / core:fullName ?uniprot_recommended_name ;
        genex:hasExpressionCondition / genex:hasSex ?expression_condition_sex ;
        genex:hasSequenceUnit / lscr:xrefUniprot / corecatalyticactivity:annotation / core:catalyticActivity / core:catalyzedReaction / rdfs:label ?rhea_label .
}
LIMIT 100
""" ;
    schema:target "https://rdfportal.org/sib/sparql".
